# Ignore Python cache files
__pycache__/
*.py[cod]
*$py.class

# Ignore virtual environment
venv/
env/
.env

# Ignore OS files
.DS_Store
Thumbs.db

# Ignore IDE files
.vscode/
.idea/
*.swp
*.swo

# Ignore temporary files
*.tmp
*.temp
tmp*

# Ignore output directory (will be created during runtime)
output/

# Ignore logs
*.log

# Ignore Jupyter notebooks checkpoints
.ipynb_checkpoints/

# Ignore pytest cache
.pytest_cache/

# Ignore coverage reports
htmlcov/
.coverage
.coverage.*
